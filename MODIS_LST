   "source": [
    "yr = 2004\n",
    "mo = 8\n",
    "\n",
    "fi_terra = get_tile_files(ds_modA2,yr,mo)\n",
    "fi_aqua = get_tile_files(ds_mydA2,yr,mo)\n",
    "\n",
    "def wrap_tile_process(ti):\n",
    "    #if os.path.isfile('./res/'+str(ti).zfill(4)+'_d.tif')==False:\n",
    "    try:\n",
    "        tile_process(fi_terra,fi_aqua,ti)\n",
